        "coveralls": "^3.0.3",
        "cross-env": "^5.2.0",
        "enzyme": "^3.8.0",
        "enzyme-adapter-react-16": "^1.7.1",
        "factory.ts": "^0.4.3",
        "husky": "^1.3.1",
